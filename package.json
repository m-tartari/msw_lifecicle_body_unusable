{
  "name": "msw_test",
  "version": "v1.0.0",
  "description": "msw_test",
  "author": "m-tartari",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:storybook": "storybook build --output-dir ./docs",
    "ci:build": "npm run build",
    "ci:build:storybook": "npm run build:storybook",
    "ci:test:coverage": "npm run test:coverage  --detectOpenHandles --forceExit",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix",
    "serve": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:coverage": "vitest --coverage .",
    "test:coverage:visual-regression": "vitest  --coverage --coverageDirectory=coverage/visual-regression --roots=\"<rootDir>\" --testMatch=\"<rootDir>/src/components/*\""
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.0",
    "@tanstack/react-query": "^5.59.15",
    "@vitejs/plugin-react": "^4.3.2",
    "axios": "^1.7.7",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "vite": "^5.4.8",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.0.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.17.10",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitest/coverage-istanbul": "^2.1.2",
    "@vitest/eslint-plugin": "^1.1.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.9.0",
    "eslint-plugin-testing-library": "^6.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^25.0.1",
    "jsdom-worker": "^0.3.0",
    "msw": "^2.4.9",
    "prettier": "^3.3.3",
    "react-test-renderer": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vitest": "^2.1.2",
    "vitest-canvas-mock": "^0.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
